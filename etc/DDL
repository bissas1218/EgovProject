DROP TABLE example.menu;

CREATE TABLE `menu` (
  `MENU_ID` varchar(10) NOT NULL COMMENT '메뉴아이디',
  `P_MENU_ID` varchar(10) NOT NULL,
  `MENU_NM` varchar(30) NOT NULL,
  `DEPTH` int(11) NOT NULL,
  `MENU_ORDER` int(11) NOT NULL,
  `MENU_TYPE` varchar(10) NOT NULL,
  `TYPE_VAL` varchar(10) DEFAULT NULL,
  UNIQUE KEY `menu_MENU_ID_IDX` (`MENU_ID`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci COMMENT='메뉴관리';

INSERT INTO example.menu
(MENU_ID, P_MENU_ID, MENU_NM, `DEPTH`, MENU_ORDER, MENU_TYPE)
VALUES('M-0001', '#', 'My Homepage', 0, 0, 'directory');


DROP TABLE example.board;

CREATE TABLE `board` (
  `BOARD_ID` varchar(10) NOT NULL,
  `BOARD_NM` varchar(20) NOT NULL,
  `BOARD_TYPE` varchar(10) NOT NULL,
  `BOARD_DESCR_YN` char(1) NOT NULL DEFAULT 'N',
  `BOARD_DESCR` varchar(1000) DEFAULT NULL,
  UNIQUE KEY `board_BOARD_ID_IDX` (`BOARD_ID`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci COMMENT='게시판관리';

DROP TABLE example.normal_board;

CREATE TABLE `normal_board` (
  `NOR_BRD_ID` varchar(10) NOT NULL,
  `BOARD_ID` varchar(10) NOT NULL,
  `POST_TITLE` varchar(50) NOT NULL,
  `POST_CONTENT` longtext NOT NULL,
  UNIQUE KEY `NORMAL_BOARD_NOR_BRD_ID_IDX` (`NOR_BRD_ID`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci COMMENT='일반게시판게시물';

DROP TABLE example.contents;

CREATE TABLE `contents` (
  `CONTENTS_ID` varchar(10) NOT NULL,
  `CONTENTS_NM` varchar(50) NOT NULL,
  `CONTENTS_TEXT` longtext NOT NULL,
  `REG_DATE` datetime NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci COMMENT='컨텐츠';

DROP TABLE example.photo_board;

CREATE TABLE `photo_board` (
  `PHOTO_ID` varchar(10) NOT NULL,
  `SAVE_NM` varchar(50) DEFAULT NULL,
  `TYPE` varchar(10) DEFAULT NULL,
  `SIZE` varchar(10) DEFAULT NULL,
  `REAL_NM` varchar(50) DEFAULT NULL,
  `SAVE_PATH` varchar(1000) DEFAULT NULL,
  `TITLE` varchar(50) DEFAULT NULL,
  `DESCR` varchar(100) DEFAULT NULL,
  `REG_DATE` datetime DEFAULT NULL,
  `BOARD_ID` varchar(10) DEFAULT NULL,
  UNIQUE KEY `photo_board_PHOTO_ID_IDX` (`PHOTO_ID`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci COMMENT='사진게시판';

DROP TABLE example.video_board;

CREATE TABLE `video_board` (
  `VIDEO_ID` varchar(10) NOT NULL,
  `SAVE_NM` varchar(100) DEFAULT NULL,
  `TYPE` varchar(10) DEFAULT NULL,
  `SIZE` varchar(10) DEFAULT NULL,
  `REAL_NM` varchar(100) DEFAULT NULL,
  `SAVE_PATH` varchar(1000) DEFAULT NULL,
  `TITLE` varchar(50) DEFAULT NULL,
  `DESCR` varchar(100) DEFAULT NULL,
  `REG_DATE` datetime DEFAULT NULL,
  `BOARD_ID` varchar(10) DEFAULT NULL,
  UNIQUE KEY `video_board_VIDEO_ID_IDX` (`VIDEO_ID`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci COMMENT='동영상게시판';

DROP TABLE example.program;

CREATE TABLE `program` (
  `PROGRAM_ID` varchar(7) NOT NULL,
  `URL` varchar(30) NOT NULL,
  `PROGRAM_NM` varchar(50) NOT NULL,
  UNIQUE KEY `program_PROGRAM_ID_IDX` (`PROGRAM_ID`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci COMMENT='프로그램관리';

DROP TABLE example.schedule;

CREATE TABLE `schedule` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `s_date` varchar(8) NOT NULL,
  `title` varchar(50) DEFAULT NULL,
  `srt_time` varchar(6) DEFAULT NULL,
  `end_time` varchar(6) DEFAULT NULL,
  `holiday_yn` char(1) NOT NULL DEFAULT 'N',
  UNIQUE KEY `schedule_id_IDX` (`id`) USING BTREE
) ENGINE=InnoDB AUTO_INCREMENT=111 DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci COMMENT='일정관리';

DROP TABLE example.golf_reserv;

CREATE TABLE `golf_reserv` (
  `date` varchar(8) DEFAULT NULL,
  `course` varchar(1) DEFAULT NULL,
  `time` varchar(5) DEFAULT NULL,
  `hole` varchar(2) DEFAULT NULL,
  `caddy` varchar(1) DEFAULT NULL,
  `person` int(11) DEFAULT NULL,
  `green_fee` int(11) DEFAULT NULL,
  `part` varchar(1) DEFAULT NULL,
  `holiday_yn` varchar(1) DEFAULT NULL,
  `member_type` varchar(1) DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

------- event ----------
CREATE TABLE `test_table` (
  `TEST1` varchar(100) DEFAULT NULL,
  `TEST2` datetime DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

CREATE EVENT TEST_EVENT
ON SCHEDULE EVERY 1 day
           STARTS '2023-07-26 17:48:00' 
    ON COMPLETION NOT PRESERVE
    ENABLE
DO INSERT INTO TEST_TABLE (TEST1, TEST2) VALUES ('테스트', NOW())